# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=30000

# Environment
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# WebSocket
REACT_APP_WS_URL=ws://localhost:5000

# Features
REACT_APP_ENABLE_SOUND_DETECTION=true
REACT_APP_ENABLE_FACE_DETECTION=true
REACT_APP_ENABLE_PHONE_DETECTION=true
REACT_APP_ENABLE_EYE_GAZE_TRACKING=true
REACT_APP_ENABLE_BACKGROUND_BLUR=true
REACT_APP_ENABLE_TAB_MONITORING=true

# Violation Settings
REACT_APP_TRUST_SCORE_THRESHOLD=50
REACT_APP_VIOLATION_THRESHOLD=10
REACT_APP_AUTO_SUBMIT_ENABLED=true